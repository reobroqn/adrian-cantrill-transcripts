{
    "name": "adrian-transcript-automation",
    "version": "1.0.0",
    "description": "Browser automation for collecting Adrian Cantrill course transcripts",
    "main": "dist/automate.js",
    "scripts": {
        "build": "tsc",
        "start": "node --env-file=.env -r ts-node/register src/Player.ts",
        "dev": "node --env-file=.env -r ts-node/register src/Player.ts --debug",
        "scrape": "node --env-file=.env -r ts-node/register src/Scraper.ts",
        "play": "node --env-file=.env -r ts-node/register src/Player.ts",
        "install-deps": "npm install"
    },
    "dependencies": {
        "puppeteer": "^24.33.0",
        "puppeteer-extra": "^3.3.6",
        "puppeteer-extra-plugin-stealth": "^2.11.2"
    },
    "keywords": [
        "automation",
        "transcript",
        "puppeteer"
    ],
    "author": "",
    "license": "MIT",
    "type": "commonjs",
    "devDependencies": {
        "@types/node": "^25.0.2",
        "@types/puppeteer": "^5.4.7",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    }
}